{
  "name": "app",
  "private": "true",
  "scripts": {
    "build": "esbuild app/javascript/application.js --bundle --sourcemap --outdir=app/assets/builds --loader:.js=jsx",
    "watch": "esbuild app/javascript/*.* --watch --bundle --outdir=app/assets/builds --loader:.js=jsx"
  },
  "dependencies": {
    "esbuild": "^0.14.43",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0"
  }
}
